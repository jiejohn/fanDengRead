.loading {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.loading .inbox {
  width: 67.5rpx;
  height: 67.5rpx;
  text-align: center;
  background-color: rgba(23, 23, 23, 0.9);
  border-radius: 8.1rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
@media (max-width: 1920px) {
  .loading .inbox {
    width: 97.5rpx;
    height: 97.5rpx;
    border-radius: 11.7rpx;
  }
}
.loading .inbox .loadimg {
  width: 27rpx;
  height: 27rpx;
  display: block;
  margin: 0 auto;
  background: url("data:image/png;base64,") 0 0 no-repeat;
  background-size: 100% 100%;
  animation: move 0.8s infinite linear;
  -webkit-animation: move 0.8s infinite linear;
}
@media (max-width: 1920px) {
  .loading .inbox .loadimg {
    width: 39rpx;
    height: 39rpx;
  }
}
.loading .inbox text {
  font-size: 10.8rpx;
  margin-top: 13.5rpx;
  text-align: center;
  color: #fff;
  line-height: 1;
  display: block;
}
@media (max-width: 1920px) {
  .loading .inbox text {
    font-size: 15.6rpx;
    margin-top: 19.5rpx;
  }
}
@keyframes move {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
